cnnzite_mod.init();
/* append ie class if less than version 9 */
if ($j.browser.msie) {
if(parseInt($j.browser.version,10) && (parseInt($j.browser.version,10) <= 8)) {
$j("body").addClass('cnn_oldIE');
}
}
/* check for touch enabled device */
if ( Modernizr.touch ) {
$j("body").addClass('cnn_touch');
}
});
</script>
